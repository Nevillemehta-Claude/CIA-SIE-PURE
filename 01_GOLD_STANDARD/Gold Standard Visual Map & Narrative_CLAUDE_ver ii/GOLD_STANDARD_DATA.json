  "lifecycleModules": {
    "id": "LIFECYCLE_MODULES",
    "description": "Complete development pathway from idea to operations",

    "phases": [
      {
        "number": 1,
        "id": "GS-LM-001",
        "name": "Ideation",
        "stream": "CONCEPTION",
        "purpose": "Capture the initial spark of an idea",
        "deliverables": ["Idea Statement", "Problem Statement", "Success Vision", "Initial Constraints"],
        "exitCriteria": "Principal confirms: 'This is what I want to explore.'"
      },
      {
        "number": 2,
        "id": "GS-LM-002",
        "name": "Concept Crystallization",
        "stream": "CONCEPTION",
        "purpose": "Transform idea into defined concept with clear boundaries",
        "deliverables": ["Concept Document", "Scope Definition", "Component Overview", "Risk Register", "Success Criteria"],
        "exitCriteria": "Principal confirms: 'This is the shape of what we're building.'"
      },
      {
        "number": 3,
        "id": "GS-LM-003",
        "name": "Requirements Analysis",
        "stream": "CONCEPTION",
        "purpose": "Define exactly what the system must do",
        "deliverables": ["Requirements Specification (REQ-XXX IDs)", "MoSCoW Prioritization", "RTM", "Acceptance Criteria"],
        "exitCriteria": "No TBD or ambiguous requirements remain."
      },
      {
        "number": 4,
        "id": "GS-LM-004",
        "name": "Architecture Design",
        "stream": "DESIGN",
        "purpose": "Define structural solution to meet requirements",
        "deliverables": ["C4 Diagrams (Context, Container)", "ADRs", "Quality Attributes", "Technology Selection"],
        "exitCriteria": "Architecture reviewed and approved."
      },
      {
        "number": 5,
        "id": "GS-LM-005",
        "name": "Detailed Specification",
        "stream": "DESIGN",
        "purpose": "Define precise technical blueprint for implementation",
        "deliverables": ["Component Specs (C4 Level 3)", "Interface Specs (OpenAPI/AsyncAPI)", "Data Models", "State Machines", "Error Handling", "UI/UX Specs"],
        "exitCriteria": "All components specified. Developer can implement without questions."
      },
      {
        "number": 6,
        "id": "GS-LM-006",
        "name": "Development",
        "stream": "CONSTRUCTION",
        "purpose": "Build components according to specification",
        "deliverables": ["Working Code", "Unit Tests (>90% coverage)", "Contract Tests (100% interfaces)", "Code Reviews", "CI/CD Pipeline"],
        "exitCriteria": "All components implemented. All tests pass. CI pipeline green."
      },
      {
        "number": 7,
        "id": "GS-LM-007",
        "name": "Integration",
        "stream": "CONSTRUCTION",
        "purpose": "Connect components into working system",
        "deliverables": ["Integration Matrix", "Dependency Graph", "Integration Tests", "Working System"],
        "exitCriteria": "All components integrated. No critical defects."
      },
      {
        "number": 8,
        "id": "GS-LM-008",
        "name": "Verification",
        "stream": "DELIVERY",
        "purpose": "Prove system works as specified",
        "deliverables": ["Full Test Pyramid", "Performance Results (<200ms p95)", "Security Scan", "RTM Complete", "UAT Sign-off"],
        "exitCriteria": "All tests passing. UAT approved."
      },
      {
        "number": 9,
        "id": "GS-LM-009",
        "name": "Security Review",
        "stream": "DELIVERY",
        "purpose": "Verify system security before production",
        "deliverables": ["Auth Review", "OWASP Top 10", "SAST/DAST Scans", "Vulnerability Assessment", "Security Sign-off"],
        "exitCriteria": "Security sign-off obtained. No critical vulnerabilities."
      },
      {
        "number": 10,
        "id": "GS-LM-010",
        "name": "Deployment",
        "stream": "DELIVERY",
        "purpose": "Safely release to production",
        "deliverables": ["Release Artifacts", "Deployment Plan (blue-green/canary/rolling)", "Rollback Plan", "Health Checks", "Smoke Tests"],
        "exitCriteria": "Deployment successful. All health checks passing. Stakeholders notified."
      },
      {
        "number": 11,
        "id": "GS-LM-011",
        "name": "Operations",
        "stream": "OPERATION",
        "purpose": "Maintain and evolve system in production",
        "activities": ["Monitoring 24/7", "Incident Response", "Runbooks", "On-Call Rotation", "Postmortems", "SLO Dashboards", "Continuous Improvement"],
        "exitCriteria": "No exit - continuous phase."
      }
    ]
  },

