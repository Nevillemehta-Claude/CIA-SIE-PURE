  "coreKernel": {
    "id": "CORE_KERNEL",
    "description": "Immutable foundation principles that cannot be modified or overridden",

    "firstPrinciples": {
      "id": "GS-CK-001",
      "title": "First Principles",
      "category": "AXIOMS",

      "eightPrinciples": [
        {
          "number": 1,
          "name": "NASA-Style Rigour",
          "description": "Apply aerospace-grade discipline to software development. Every decision documented, every assumption validated, every outcome verified."
        },
        {
          "number": 2,
          "name": "Audit Before Build",
          "description": "Understand the current state completely before making changes. No assumptions without evidence."
        },
        {
          "number": 3,
          "name": "Zero Drift Policy",
          "description": "Documentation and implementation must remain synchronized. Drift is debt; debt compounds."
        },
        {
          "number": 4,
          "name": "Living Documentation",
          "description": "Documentation is not an afterthought. It evolves with the code, tested alongside the code."
        },
        {
          "number": 5,
          "name": "Full Bidirectional Traceability",
          "description": "Every requirement traces to implementation. Every implementation traces to requirement. No orphans."
        },
        {
          "number": 6,
          "name": "Evidence-Based Validation",
          "description": "Assertions require proof. Tests are evidence. Passing tests are the only acceptable evidence."
        },
        {
          "number": 7,
          "name": "Single Source of Truth (SSOT)",
          "description": "One authoritative source per artifact. Duplication is divergence waiting to happen."
        },
        {
          "number": 8,
          "name": "Defensive Documentation",
          "description": "Document for the skeptic. Assume the reader questions everything. Provide evidence for every claim."
        }
      ],

      "fiveImmutableLaws": [
        {
          "number": 1,
          "name": "Meaning Precedes Implementation",
          "meaning": "Understand what before how. Define semantics before syntax."
        },
        {
          "number": 2,
          "name": "Structure Precedes Intelligence",
          "meaning": "Get the architecture right before adding complexity."
        },
        {
          "number": 3,
          "name": "Validation Precedes Optimization",
          "meaning": "Make it work, make it right, then make it fast."
        },
        {
          "number": 4,
          "name": "Explicit Precedes Implicit",
          "meaning": "No magic. No hidden behavior. Everything visible and documented."
        },
        {
          "number": 5,
          "name": "Reversibility Precedes Commitment",
          "meaning": "Every decision should be reversible until proven correct."
        }
      ],

      "sixArchitecturePrinciples": [
        {"number": 1, "name": "Single Source of Truth (SSOT)", "description": "One authoritative source per data element"},
        {"number": 2, "name": "Loose Coupling", "description": "Components interact through well-defined interfaces"},
        {"number": 3, "name": "High Cohesion", "description": "Related functionality grouped together"},
        {"number": 4, "name": "Explicit Contracts", "description": "All interfaces formally specified"},
        {"number": 5, "name": "Fail-Safe Defaults", "description": "System fails to safe state"},
        {"number": 6, "name": "Deterministic Behavior", "description": "Same input always produces same output"}
      ]
    },

    "modularityDoctrine": {
      "id": "GS-CK-002",
      "title": "Modularity Doctrine",
      "category": "ARCHITECTURE",

      "sevenProperties": [
        {"property": "Self-Contained", "definition": "All dependencies explicit and internal", "test": "Can be understood without external context"},
        {"property": "Independently Testable", "definition": "Can be tested in isolation", "test": "Has its own test suite that runs independently"},
        {"property": "Versioned", "definition": "Has explicit version number", "test": "Version changes when interface changes"},
        {"property": "Documented", "definition": "Purpose, interface, and behavior documented", "test": "New developer can use without asking questions"},
        {"property": "Interface-Defined", "definition": "Interaction only through defined interface", "test": "No direct access to internals"},
        {"property": "Replaceable", "definition": "Can be swapped for alternative implementation", "test": "Interface remains stable across implementations"},
        {"property": "Evolvable", "definition": "Can be updated without affecting dependents", "test": "Internal changes don't break consumers"}
      ],

      "antiPatterns": [
        {"name": "Hidden Dependencies", "description": "Undeclared requirements on external components"},
        {"name": "Circular Dependencies", "description": "A depends on B depends on A"},
        {"name": "God Module", "description": "One module that does everything"},
        {"name": "Leaky Abstraction", "description": "Internal details exposed through interface"},
        {"name": "Version Lock", "description": "Rigid version requirements preventing updates"}
      ]
    },

    "verificationDoctrine": {
      "id": "GS-CK-003",
      "title": "Verification Doctrine",
      "category": "TESTING",

      "tenLayers": [
        {"layer": 10, "name": "ACCEPTANCE", "question": "Does it meet business goals?"},
        {"layer": 9, "name": "SECURITY", "question": "Are there vulnerabilities?"},
        {"layer": 8, "name": "PERFORMANCE", "question": "Is it within acceptable bounds?"},
        {"layer": 7, "name": "END-TO-END", "question": "Do complete workflows work?"},
        {"layer": 6, "name": "FUNCTIONAL", "question": "Do features work correctly?"},
        {"layer": 5, "name": "REGRESSION", "question": "Did we break anything?"},
        {"layer": 4, "name": "INTEGRATION", "question": "Do components connect properly?"},
        {"layer": 3, "name": "CONTRACT", "question": "Are API contracts honored?"},
        {"layer": 2, "name": "UNIT", "question": "Do individual functions work?"},
        {"layer": 1, "name": "SYNTAX", "question": "Does the code compile/run?"}
      ],

      "coverageTargets": [
        {"category": "Safety-Critical", "target": "100%", "rationale": "No untested paths in critical code"},
        {"category": "Business-Critical", "target": "95%", "rationale": "Core business logic fully covered"},
        {"category": "Standard", "target": "90%", "rationale": "Comprehensive coverage for normal code"},
        {"category": "Utility", "target": "80%", "rationale": "Helper functions adequately tested"}
      ],

      "testPyramid": [
        {"type": "Unit Tests", "percentage": 50, "characteristics": "Fast, isolated, deterministic"},
        {"type": "Contract Tests", "percentage": 20, "characteristics": "API boundary verification"},
        {"type": "Integration Tests", "percentage": 20, "characteristics": "Component interaction"},
        {"type": "E2E Tests", "percentage": 10, "characteristics": "Full user workflows"}
      ],

      "rule": "Lower layers must pass before higher layers are tested"
    }
  },

  "commandProtocol": {
    "id": "COMMAND_PROTOCOL",
    "description": "Session control and authority documents",

    "genesis": {
      "id": "GS-CP-001",
      "title": "Genesis Protocol",
      "category": "START",

      "identity": {
        "principal": "Neville Mehta",
        "role": "Product Owner, Standards Authority, Quality Arbiter",
        "authorityLevel": "Final decision on all matters of implementation, architecture, and quality"
      },

      "threeAbsolutes": [
        {
          "name": "Accuracy Over Speed",
          "quote": "Shortcuts are an illusion of speed and the bane of accuracy.",
          "meaning": "Every output must be correct. Rushed work that requires rework is slower than careful work done once."
        },
        {
          "name": "Modularity Is Law",
          "quote": "Every worthy system is composed of autonomous, self-contained units.",
          "meaning": "Code, documentation, and processes must be modular - independently developable, testable, upgradeable, and replaceable."
        },
        {
          "name": "Verification Is Continuous",
          "quote": "Testing is not a phase. It is a constant companion to every action.",
          "meaning": "Every artifact produced must be verified before the next artifact begins. No layer can be skipped."
        }
      ],

      "rulesOfEngagement": {
        "mustDo": [
          "Read before acting - Load the relevant phase playbook before implementation",
          "Verify continuously - Check work against phase verification criteria",
          "Document as you go - Living documentation, not retrospective",
          "State assumptions explicitly - No hidden logic, no magic",
          "Flag uncertainties - When unsure, state confidence level and rationale"
        ],
        "neverDo": [
          "Never skip verification layers",
          "Never make undocumented assumptions",
          "Never proceed past a failed gate",
          "Never optimize before correct",
          "Never violate module boundaries"
        ]
      },

      "qualityExpectations": [
        {"dimension": "Correctness", "standard": "100% functional accuracy"},
        {"dimension": "Coverage", "standard": "Every requirement implemented"},
        {"dimension": "Verification", "standard": "Every implementation tested"},
        {"dimension": "Documentation", "standard": "Every public interface documented"},
        {"dimension": "Traceability", "standard": "Every artifact traceable to source"}
      ],

      "communicationTemplates": {
        "whenUncertain": "I am [X]% confident in this approach because [rationale]. My assumption is [assumption]. Please confirm or redirect.",
        "whenBlocked": "I cannot proceed because [blocker]. Options are: [A], [B], [C]. My recommendation is [X] because [rationale].",
        "whenComplete": "Phase [X] complete. Verification criteria satisfied: [list]. Ready for: [next phase or review]."
      }
    },

